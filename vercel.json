{
  "version": 2,
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/controllers/server.js" },
    { "src": "/(.*)", "dest": "client/next-tutorial/next.config.mjs" }
  ]
}
